<html>
  <head>

   <% include ../partials/head %>
   <link rel='stylesheet' href='/css/extract.css' />
 
  </head>
    <body>
    <nav class="navbar navbar-inverse">
    <div class="container-fluid">
      <% if(loggedin == 'true'){ %>
        <div class="navbar-header">
          <a class="navbar-brand" href="/logout">Logout</a>
        </div>
      <% } else{ %>  
        <div class="navbar-header">
          <a class="navbar-brand" href="/login">Login</a>
        </div>
      <% } %>
      <div>
        <ul class="nav navbar-nav">
          <li><a href="/search">Search</a></li>
          <li class="active"><a href="/extract">Extract</a></li>
        </ul>
      </div>
    </div>
  </nav>
<div class="container">
    <form name="uploader" action="/fileupload" method="post" enctype="multipart/form-data">
      <div class="form-inline">
        <div class="form-group">
          <input type="file" name="file" id="file"/>
        </div>
        <div class="form-group" id="gender">
            <label class="radio-inline"><input type="radio" name="optradio" checked="" value="public">Public</label>
            <label class="radio-inline"><input type="radio" name="optradio" value="private">Private</label>
        </div>
       
          <button type="submit" class="btn btn-sm btn-primary" id="js-upload-submit">Upload files</button></div>
      </div>

      </form>
</div> <!-- /container -->
    </body>
    </html>